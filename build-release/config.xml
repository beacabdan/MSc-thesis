<?xml version="1.0" ?>
<config>  
	<!-- Global simulation parameters -->
	<output resultsFile="./data/randomWalkers.h5" logsDir="./logs"/>
	<numSteps value="10" serializeResolution="1"/>
    <learningLoop iterations="10" rollouts="30"/>
	<size width="5" height="5"/>
	<numAgents value="4"/>
    <extrarewards num="3" size="1" x1="0" y1="0" x2="2" y2="2" x3="1" y3="1" x4="1" y4="1"/>
    <numBasis xrange="4" yrange="0" sigma="1.0"/>
    
    <lambda value="0.7"/>
    <learningRate value="0.2"/>
    <gamma value="0.2"/> <!--0.2 for payoff dominant, 0.9 for risk dominant-->
</config>
